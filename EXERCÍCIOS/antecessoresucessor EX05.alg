algoritmo "antecessoresucessor"
"Exercício V"
var
valint, ant, suc: inteiro
inicio

escreval("Mostrar o sucessor e antecessor")

escreval("Digite um valor inteiro: ")
leia(valint)

ant <- valint - 1
suc <- valint + 1

escreval("O valor digitado é ", valint,", seu antecessor é ", ant, " e seu sucessor é ", suc)

fimalgoritmo
